{
    "server": {
        "address": "::",
        "path": "/var/run/activitypub-fw.sock",
        "port": "8080",
        "protocol": "unix",
        "readTimeout": 5,
        "writeTimeout": 10,
        "idleTimeout": 120,
        "inboundProxyNetworks": [
            "127.0.0.1",
            "::1"
        ]
    },
    "proxy": {
        "protocol": "unix",
        "url": "http://",
        "unixPath": "/var/run/nginx.sock",
        "forceAttemptHttp2": true,
        "maxConnsPerHost": 256,
        "maxIdleConns": 100,
        "maxIdleConnsPerHost": 5,
        "idleConnTimeout": 120,
        "tlsHandshakeTimeout": 10,
        "expectContinueTimeout": 1,
        "keepAlive": 30,
        "timeout": 30,
        "writeBufferSize": 4096,
        "readBufferSize": 4096
    },
    "limit": {
        "maxBodySize": 1048576,
        "cc": 6,
        "mentions": 5,
        "keywords": []
    }
}